@model Financeiro.Models.bancos
<div class="modal-header pigendo-secondary">
    <h5 class="modal-title">Deletar conta</h5>
    <button type="button" class="close" data-dismiss="modal">
        <span class="fa fa-close" style="color:red"></span>
    </button>
</div>

<div class="modal-body pigendo-primary">

    <a style="text-align:center;color:black">Deseja realmente deletar essa conta?</a>
</div>
<div class="modal-footer pigendo-secondary">
    <input type="submit" value="Delete" class="btn btn-danger btn-sm" />
</div>